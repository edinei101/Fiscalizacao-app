{
  "expo": {
    "name": "Ficalizacao",
    "slug": "ficalizacao",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.suaempresa.ficalizacao",
      "buildNumber": "1.0.0"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.suaempresa.ficalizacao",
      "versionCode": 1,
      "permissions": [
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "CAMERA",
        "RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Permitir o Ficalizacao a acessar sua c√¢mera?"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Permitir o Ficalizacao a acessar suas fotos?"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "sua-project-id-aqui"
      }
    }
  }
}